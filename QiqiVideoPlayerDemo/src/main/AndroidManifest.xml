<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.woodpecker.qiqivideoplayer">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.MANAGE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />


    <application
        android:name="com.woodpecker.qiqivideoplayer.QiqiApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:configChanges="locale"
        android:networkSecurityConfig="@xml/network_security_config"
        android:requestLegacyExternalStorage="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning"
        tools:targetApi="n">
        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"
            android:exported="true"

            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.TestListActivity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="fullSensor"/>
        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.TestFullActivity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"/>

        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.TestSurfaceActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="false"
            android:screenOrientation="portrait"/>

        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.NormalVideoActivity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"/>
        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.MultipleActivity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"/>
        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.TikTok2Activity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"/>
        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.TikTok1Activity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"/>

        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.BulletScreenActivity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"/>
        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.AdActivity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"/>
        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.ContinuousVideoActivity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"/>
        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.ClarityActivity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"/>
        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.TinyScreenActivity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"/>
        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.PipActivity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"/>
        <activity android:name="com.woodpecker.qiqivideoplayer.newPlayer.activity.PipListActivity"
            android:exported="false"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"/>


        <activity android:name="com.woodpecker.qiqivideoplayer.music.MusicPlayerActivity"
            android:exported="false"/>
        <activity android:name="com.woodpecker.qiqivideoplayer.m3u8.M3u8Activity"
            android:exported="false"/>
        <activity android:name="com.frank.ffmpeg.activity.VideoPreviewActivity"
            android:exported="false"/>
        <activity android:name="com.frank.ffmpeg.activity.VideoHandleActivity"
            android:exported="false"/>
        <activity android:name="com.frank.ffmpeg.activity.FilterActivity"
            android:exported="false"/>
    </application>

</manifest>